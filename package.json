{
  "name": "react_redux_base",
  "version": "0.1.0",
  "description": "Environment with Redux and React to start an app",
  "engines": {
    "npm": ">3",
    "node": ">6"
  },
  "scripts": {
    "preinstall": "node tools/nodeVersionCheck.js",
    "start-message": "babel-node tools/startMessage.js",
    "prestart": "npm-run-all --parallel start-message remove-dist",
    "start": "npm-run-all --parallel open:src lint:watch",
    "open:src": "babel-node tools/srcServer.js",
    "open:dist": "babel-node tools/distServer.js",
    "lint": "esw webpack.config.* src tools --color && npm run lint:scss && npm run lint:styled-components",
    "lint:scss": "stylelint \"src/styles/**/*.scss\" --config .scss.stylelintrc",
    "lint:styled-components": "stylelint './src/**/*.js'",
    "lint:watch": "npm run lint -- --watch",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "rimraf ./dist",
    "prebuild": "npm run clean-dist && npm run lint && npm run test",
    "build": "npm run just-build && npm run open:dist",
    "prejust-build": "npm run clean-dist",
    "just-build": "babel-node tools/build.js",
    "test": "jest --setupTestFrameworkScriptFile ./tools/testSetup",
    "test:CI": "node --harmony_proxies node_modules/jest/bin/jest",
    "test:cover": "npm run test -- --coverage",
    "test:cover:CI": "npm run test:cover && cat ./coverage/lcov.info | node_modules/coveralls/bin/coveralls.js",
    "test:watch": "npm run test -- --watch",
    "open:cover": "npm run test:cover && open coverage/lcov-report/index.html",
    "deploy": "npm run just-build && babel-node tools/deployS3",
    "analyze-bundle": "babel-node ./tools/analyzeBundle.js"
  },
  "author": "Bernabe Gonzalez",
  "license": "MIT",
  "dependencies": {
    "history": "4.7.2",
    "humps": "2.0.1",
    "immutable": "3.8.2",
    "isomorphic-fetch": "2.2.1",
    "lodash": "4.17.4",
    "prop-types": "15.6.0",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-redux": "5.0.6",
    "react-router-dom": "4.2.2",
    "react-router-redux": "5.0.0-alpha.9",
    "redux": "3.7.2",
    "redux-form": "7.2.0",
    "redux-immutable": "4.0.0",
    "redux-logger": "3.0.6",
    "redux-react-session": "2.2.1",
    "redux-thunk": "2.2.0",
    "styled-components": "2.3.0",
    "validate.js": "0.11.1"
  },
  "devDependencies": {
    "autoprefixer": "7.2.3",
    "aws-sdk": "2.126.0",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-eslint": "8.0.3",
    "babel-jest": "21.2.0",
    "babel-loader": "7.1.2",
    "babel-plugin-transform-react-constant-elements": "6.23.0",
    "babel-plugin-transform-react-remove-prop-types": "0.4.12",
    "babel-polyfill": "6.26.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-1": "6.24.1",
    "babel-register": "6.26.0",
    "chalk": "2.3.0",
    "compression-webpack-plugin": "1.1.2",
    "connect-history-api-fallback": "1.5.0",
    "coveralls": "3.0.0",
    "css-loader": "0.28.7",
    "dotenv": "4.0.0",
    "dotenv-webpack": "1.5.4",
    "enzyme": "3.2.0",
    "enzyme-adapter-react-16": "1.1.0",
    "eslint": "4.13.1",
    "eslint-config-airbnb": "16.1.0",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-react": "7.5.1",
    "eslint-watch": "3.1.3",
    "express": "4.16.2",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.6",
    "html-webpack-plugin": "2.30.1",
    "identity-obj-proxy": "3.0.0",
    "jest": "21.2.1",
    "json-loader": "0.5.7",
    "mime": "2.0.3",
    "nock": "9.1.4",
    "node-sass": "4.7.2",
    "npm-run-all": "4.1.2",
    "opn": "5.1.0",
    "postcss-loader": "1.3.3",
    "prompt": "1.0.0",
    "react-hot-loader": "3.1.3",
    "react-test-renderer": "16.2.0",
    "replace": "0.3.0",
    "rimraf": "2.6.2",
    "sass-loader": "6.0.6",
    "style-loader": "0.19.1",
    "stylelint": "8.4.0",
    "stylelint-config-standard": "18.0.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-order": "0.6.0",
    "stylelint-processor-styled-components": "1.2.1",
    "url-loader": "0.6.2",
    "webpack": "3.10.0",
    "webpack-bundle-analyzer": "2.9.1",
    "webpack-dev-middleware": "2.0.1",
    "webpack-hot-middleware": "2.21.0",
    "webpack-md5-hash": "0.0.6"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css|scss)$": "identity-obj-proxy",
      "^.+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga|ico)$": "<rootDir>/tools/fileMock.js"
    },
    "testURL": "http://localhost"
  },
  "keywords": [],
  "repository": {
    "type": "git",
    "url": "https://github.com/bernabe9/react-redux-base"
  }
}
